<form autocomplete="off" class="fatex-u-full-height {{cssClass}}">
    <div class="fatex-desk">
        {{!--HEADER--}}
        <div class="fatex-desk__header">
            <div class="fatex-header">
                <div class="fatex-header__name">
                    <label class="fatex-header__label fatex__item__header__label">
                        {{#if isOwnedBy}}
                            {{ isOwnedBy }}
                        {{else}}
                            {{localize label}}
                        {{/if}}
                    </label>

                    <div class="fatex-header__title fatex__item__header__title">
                        {{localize title}}
                    </div>
                </div>

                {{#if centerPartial}}
                    {{> centerPartial }}
                {{/if}}

                <img class="fatex-header__logo" src="/systems/fatex/assets/logos/powered_by_fate.svg">
            </div>
        </div>

        <div class="fatex-desk__content">
